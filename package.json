    "joi": "^17.9.2"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.36.1",
    "npm-check-updates": "^16.10.12",
    "prettier": "^2.8.8",
    "standard-version": "^9.5.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "github",
    "issues",
    "pull requests",
    "lock",
    "automation",
    "github actions",
    "project management",
    "bot"
